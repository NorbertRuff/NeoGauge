[innerEdgeStyle]
X=((#radius#*4)+#edging#)
Y=#edging#

[mainTextStyle]
X=((#radius#*4)+#edging#)
Y=(#radius#*0.1)r
StringAlign=RightBottom

[subTextStyle]
X=r
Y=r
StringAlign=RightTop

[dividerStyle]
X=#edging#*2.8

[iconStyle]
X=#edging#+(#radius#)*4.6
Y=#edging#+(#radius#)*0.6
W=(#radius#*0.8)
PreserveAspectRatio=1
ImageTint=#colorIcon#
AntiAlias=1
ImageAlpha=200
