[innerEdgeStyle]
X=#edging#
Y=#edging#

[mainTextStyle]
X=(#radius#*2)+(#edging#)
Y=(#radius#*0.1)r
StringAlign=LeftBottom

[subTextStyle]
X=(#radius#*0.1)r
Y=r
StringAlign=LeftTop

[dividerStyle]
X=(#radius#*2)+(#edging#)

[iconStyle]
X=(#radius#*0.6)+(#edging#)
Y=(#radius#*0.6)+(#edging#)
W=(#radius#*0.8)
PreserveAspectRatio=1
ImageTint=#colorIcon#
AntiAlias=1
ImageAlpha=255
